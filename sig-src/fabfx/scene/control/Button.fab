package fabfx.scene.control;

import fabfx.event.*;

public class Button[label L, label M] {

	public Button () {}

	public Button {L} (String{L} text) {}

	native public String {L} getText ();

	native public void setText {L} (String{L} text);

	native public void setOnAction {M} (EventHandler[M]{M} handler)
		where M <= {*->User}; 

	native public void fire {M} ();

}
